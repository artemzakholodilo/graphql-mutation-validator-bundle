  "type": "symfony-bundle",
  "licence": "MIT",
  "keywords": ["GraphQL", "Validation"],
  "authors": [
    {
      "name": "AssoConnect",
      "homepage": "https://www.assoconnect.com"
    },
    {
      "name": "Sylvain Fabre"
    }
